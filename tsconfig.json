{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2018",
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "sourceMap": false,
    "strict": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "compileOnSave": true,
    "lib": [
      "es6",
      "DOM"
    ],
    "paths": {},
    "types": [
      "intern",
      "node"
    ]
  },
  "exclude": []
}